   endparams
   variables 
      boolean v_isvisible
   endvariables

   ; get the visibillity 
   activate p_msexcel-&gt;"GET_VISIBLE"(v_isvisible)

   ; Toggle the visible
   if (p_isvisible != v_isvisible)
      activate p_msexcel-&gt;"SET_VISIBLE"(p_isvisible)
   else 
      message "Excel application is allready set correctly"
   endif
   return(0)
end ; 
